{
    "name": "T1080 DEV",
    "description": "트위치 프록시",
    "version": "1.4",
    "manifest_version": 3,
    "permissions": [
        "proxy",
        "declarativeNetRequest"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "m3u8",
                "enabled": true,
                "path": "m3u8.json"
            }
        ]
    }
}